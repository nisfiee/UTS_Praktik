#include<iostream>
#include<string>
using namespace std;

void main()
{
	string kata;
	cout<< " Masukkan kata : ";
	cin.sync();
	getline(cin, kata);
	cout<<endl;

	if(kata=="nol")
	{cout<< " 0" <<endl;}
	else if(kata=="satu")
	{cout<< " 1" <<endl;}
	else if(kata=="dua")
	{cout<< " 2" <<endl;}
	else if(kata=="tiga")
	{cout<< " 3" <<endl;}
	else if(kata=="empat")
	{cout<< " 4" <<endl;}
	else if(kata=="lima")
	{cout<< " 5" <<endl;}
	else if(kata=="enam")
	{cout<< " 6" <<endl;}
	else if(kata=="tujuh")
	{cout<< " 7" <<endl;}
	else if(kata=="delapan")
	{cout<< " 8" <<endl;}
	else if(kata=="sembilan")
	{cout<< " 9" <<endl;}
	else 
	{cout<< " Maaf ya, belum tahu cara cepatnya gimana :)" <<endl;}





}
